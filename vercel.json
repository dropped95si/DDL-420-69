{
  "buildCommand": "npm run build 2>/dev/null || echo 'No build needed for static UI'",
  "outputDirectory": "ui",
  "public": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/ui/index.html"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_ANON_KEY": "@supabase_anon_key"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
